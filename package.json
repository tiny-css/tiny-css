{
  "name": "@tiny-css/tiny-css",
  "version": "0.1.0",
  "description": "CSS stylesheets of @tiny-css",
  "main": "index.js",
  "scripts": {
    "dev": "sass src/scss/main.scss src/css/style.css --watch --no-source-map",
    "compile": "sass src/scss/main.scss src/css/style.css --no-source-map",
    "compile:optimized": "sass src/scss/main.scss src/css/style.css --no-source-map --style=compressed",
    "build": "rollup -c"
  },
  "keywords": [
    "css",
    "frameworks"
  ],
  "author": "KR Tirtho",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.1.0",
    "rollup": "^2.33.2",
    "sass": "^1.29.0",
    "typescript": "^4.0.5"
  }
}
